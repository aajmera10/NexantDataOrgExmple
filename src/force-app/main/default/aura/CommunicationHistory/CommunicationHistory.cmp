<aura:component controller ="CommunicationHistoryController" implements="forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="communications" type="Object[]"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <div class="communicationContainer">
    	<c:NewCommunication/>
    	<c:CommunicationsList allRecords = "{!v.communications}"/>
   	</div>

</aura:component>