    <validationRules>
        <fullName>Effective_Date_Must_Be_Before_Expiry</fullName>
        <active>true</active>
        <description>This validation rule will check that the Effective Date is set to be before Expiry Date.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK( Effective_Date__c )), 
NOT(ISBLANK( Expiry_Date__c )), 
Effective_Date__c &gt; Expiry_Date__c 
)</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>The Effective Date must be before the Expiry Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expiration_Date_Must_Be_In_Future</fullName>
        <active>true</active>
        <description>This validation rule will ensure that the Expiration Date is set in the future.</description>
        <errorConditionFormula>AND(
 NOT(ISBLANK(Expiry_Date__c)),
 Expiry_Date__c &lt; TODAY ()
)</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>You must set an expiration date in the future.</errorMessage>
    </validationRules>
